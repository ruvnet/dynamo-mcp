{
  "mcpServers": {
    "dynamo-mcp": {
      "command": "python",
      "args": [
        "-m",
        "dynamo_mcp.main",
        "--transport",
        "stdio"
      ],
      "alwaysAllow": [
        "list_templates",
        "add_template",
        "update_template",
        "remove_template",
        "discover_templates",
        "get_template_variables",
        "create_project"
      ]
    }
  }
}